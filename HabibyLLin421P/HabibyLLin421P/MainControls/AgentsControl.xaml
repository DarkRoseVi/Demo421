<UserControl x:Class="HabibyLLin421P.MainControls.AgentsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HabibyLLin421P.MainControls"
             mc:Ignorable="d" 
             d:DesignHeight="340" d:DesignWidth="240">
    <Grid Width="200" Height="300" Margin="20" Background="#FFFFE9F9">
        <Border BorderThickness="3" BorderBrush="#FFB175EE">
            <StackPanel>
                <TextBlock x:Name="AgentName" Text="NAME" HorizontalAlignment="Center"></TextBlock>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
