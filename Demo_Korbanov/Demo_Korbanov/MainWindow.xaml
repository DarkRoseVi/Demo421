<Window x:Class="Demo_Korbanov.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo_Korbanov"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="images/Глазки-save.ico"

        Title="Глазки-save" Height="500" Width="950">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" Grid.Column="0">
            <Image Source="/images/Глазки-save.png" />

            <TextBlock FontSize="15" TextAlignment="Center" Margin="0,10,0,0" >
                <Hyperlink x:Name="agents" Click="agents_Click" >Агенты</Hyperlink>
            </TextBlock>
        </StackPanel>
        <Frame x:Name="MainFrame" Grid.Column="1"></Frame>
    </Grid>
</Window>
